# Given an integer ‘n’, return first n Fibonacci numbers using a generator function.

vector<int> generateFibonacciNumbers(int n) {
     vector<int> fib;
    if(n <= 0) return fib;

    fib.push_back(0); 
    if(n == 1) return fib;

    fib.push_back(1);
    for(int i = 2; i < n; i++) {
        fib.push_back(fib[i-1] + fib[i-2]);
    }

    return fib;

}
